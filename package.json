{
  "name": "inline-block-grid",
  "version": "0.0.3",
  "description": "Simple SASS grid implemented with display:inline-block",
  "main": "index.js",
  "files": [
    "grid.scss",
    "grid.css",
    "grid.min.css"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "sass": "node-sass grid.scss grid.css",
    "sass:min": "node-sass grid.scss --output-style compressed grid.min.css",
    "build": "npm run sass && npm run sass:min",
    "release": "gh-release",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paulcpederson/inline-block-grid.git"
  },
  "keywords": [
    "css",
    "sass",
    "grid",
    "inline-block"
  ],
  "author": "Paul C Pederson <paul.c.pederson@gmail.com> (http://paulcpederson.com/)",
  "license": "Fair",
  "bugs": {
    "url": "https://github.com/paulcpederson/inline-block-grid/issues"
  },
  "homepage": "https://github.com/paulcpederson/inline-block-grid",
  "devDependencies": {
    "gh-release": "^1.1.2",
    "node-sass": "^2.0.1"
  }
}
